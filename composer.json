{
    "name": "swoft-fork/i18n",
    "type": "library",
  "version": "v2.0.11",
    "keywords": [
        "php",
        "swoole",
        "swoft",
        "i18n",
        "translation"
    ],
    "description": "swoft i18n component",
  "homepage": "https://github.com/sky-up/swoft-i18n",
  "license": "Apache-2.0",
    "require": {
        "php": ">=8.0",
        "swoft-fork/framework": "~2.0.0"
    },
    "autoload": {
        "psr-4": {
            "Swoft\\I18n\\": "src/"
        },
        "files": [
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "SwoftTest\\I18n\\Testing\\": "test/testing",
            "SwoftTest\\I18n\\Unit\\": "test/unit"
        }
    },
    "repositories": [
    ],
    "require-dev": {
        "phpunit/phpunit": "^7.5 || ^8.0"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit -c phpunit.xml"
    }
}
